{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "compilerOptions": {
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@consts/*": [
        "./src/consts/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@actions/*": [
        "./src/actions/*"
      ],
      "@views/*": [
        "./src/views/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@db/*": [
        "./src/db/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}